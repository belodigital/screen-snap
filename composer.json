{
    "name": "belodigital/screen-snap",
    "description": "This is my package screen-snap",
    "version": "0.7.0",
    "keywords": [
        "screen-snap",
        "laravel",
        "screenshot",
        "screenshot-taker",
        "screenshot-capture"
    ],
    "homepage": "https://github.com/belodigital/screen-snap",
    "license": "MIT",
    "authors": [
        {
            "role": "Developer",
            "name": "Diogo Cruz",
            "email": "dcruz@belodigital.com"
        },
        {
            "role": "Developer",
            "name": "Hugo Rodrigues",
            "email": "hrodrigues@belodigital.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0||^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-arch": "^2.7",
        "pestphp/pest-plugin-laravel": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "ScreenSnap\\": "src/",
            "ScreenSnap\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ScreenSnap\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-screen-snap --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ScreenSnap\\ScreenSnapServiceProvider"
            ],
            "aliases": {
                "ScreenSnap": "ScreenSnap\\Facades\\ScreenSnap"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
